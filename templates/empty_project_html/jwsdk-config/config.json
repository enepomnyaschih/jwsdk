{
	// input
	"packagesPath"  : "packages",
	"pagesPath"     : "pages",
	"templatesPath" : "templates", // optional, skip to disable "template" processor

	// output
	"publicPath"    : "../public",
	"buildUrl"      : "build",
	"pagesUrl"      : "pages",     // optional, skip to disable "template" processor
//	"jsonPath"      : "json",      // optional, skip to disable "json" processor
//	"snippetsPath"  : "snippets",  // optional, skip to disable "snippets" processor

	"tempPath"      : "temp",
	"tsTarget"      : "ES5",

	"urlPrefix"     : "/",
	"dynamicLoader" : false,
	"conversionLog" : false,

	// JS obfuscation settings
	"obfuscate"     : false,
//	"obfuscateDebugFormat": "_%v_", // enable to use predictable renaming pattern instead of obfuscation, useful for debugging
	"notObfuscateNamespaces": [ // PHP regular expressions
		"document", "window", "Math", "JSON", // Native
		"jQuery", "\\$"                       // jQuery
	],
	"notObfuscateSymbols": [ // PHP regular expressions
		// Native
		"call", "length", "push", "slice", "indexOf", "split", "join", "replace", "toLowerCase",
		"toUpperCase", "test", "exec", "className", "offsetWidth", "clientWidth",

		// jQuery
		"width", "height", "innerWidth", "innerHeight", "outerWidth", "outerHeight",
		"toggleClass", "addClass", "removeClass", "text", "css", "attr", "val", "prop", "append",
		"select", "focus", "blur", "trigger", "click", "bind", "unbind", "submit",
		"preventDefault", "stopPropagation", "stopImmediatePropagation", "shiftKey", "ctrlKey"
	],

	// Data URI is base64-formatted file contents in CSS files. Minor Web optimization.
	"embedDataUri"   : true,
	"dataUriMaxHits" : 1,
	"dataUriMaxSize" : 30720,

	// Custom mime-types for data URI conversion. Defaults to "image/<file_extension>".
	"mimeTypes": {
		"eot"  : "application/vnd.ms-fontobject",
		"ico"  : "image/x-icon",
		"jfif" : "image/jpeg",
		"jpe"  : "image/jpeg",
		"jpg"  : "image/jpeg",
		"otf"  : "application/x-font-opentype",
		"pcx"  : "image/x-pcx",
		"svg"  : "image/svg+xml",
		"tif"  : "image/tiff",
		"ttf"  : "application/x-font-truetype",
		"woff" : "application/font-woff"
	},

	"javaCmd" : "java"
}
